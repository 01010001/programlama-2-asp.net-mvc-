@{
    ViewData["Title"] = "Home Page";
}

<style>
    #carouselExampleIndicators {
        width: 100%;
        height: 50vh; /* 50% of the viewport height */
    }

    .carousel-inner {
        height: 100%;
    }

    .carousel-item {
        height: 100%;
    }

    .carousel-item img {
        object-fit: cover;
        object-position: top;
        width: 100%;
        height: 100%;
    }
</style>

